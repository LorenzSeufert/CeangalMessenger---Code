{
  "name": "electron-frontend",
  "version": "1.0.0",
  "author": "Ceangal-Team",
  "description": "Desktop app for Ceangal-Messenger",
  "keywords": [],
  "main": "main.js",
  "license": "ISC",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "devDependencies": {
    "electron": "^10.4.2",
    "electron-builder": "^22.11.1",
    "electron-reload": "^1.5.0",
    "jquery": "^3.5.1",
    "sockjs-client": "^1.5.1"
  },
  "dependencies": {
    "@stomp/stompjs": "^6.1.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-ws": "^5.0.2",
    "sockjs": "^0.3.21",
    "stompjs": "^2.3.3",
    "sweetalert2": "^11.0.17",
    "websocket": "^1.0.34",
    "webstomp-client": "^1.2.6"
  },
  "build": {
    "appId": "de.dhbw.ceangalmessenger",
    "productName": "CeangalMessenger",
    "win": {
      "target": "portable",
      "icon": "src/img/CeangalIcon.ico"
    },
    "directories": {
      "buildResources": "Electron-Frontend/src"
    },
    "extraFiles": [
      {
        "from": "src/img",
        "to": "src/img",
        "filter": [
          "**/*"
        ]
      }
    ]
  }
}
